{
  "redirects": [
    {
      "source": "/:path*",
      "has": [{ "type": "host", "value": "360wraps.ae" }],
      "destination": "https://www.360wraps.ae/:path*",
      "statusCode": 301
    },
    {
      "source": "/index.html",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/index",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/:path*/index.html",
      "destination": "/:path*/",
      "permanent": true
    }
  ]
}
